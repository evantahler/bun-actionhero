{
  "name": "keryx-example",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "license": "MIT",
  "description": "Keryx example application",
  "bin": {
    "keryx": "keryx.ts"
  },
  "scripts": {
    "start": "bun keryx.ts start",
    "dev": "bun --watch keryx.ts start",
    "migrations": "bun run migrations.ts",
    "compile": "bun build keryx.ts --compile --outfile keryx",
    "lint": "tsc && prettier --check .",
    "format": "tsc && prettier --write ."
  },
  "dependencies": {
    "keryx": "workspace:*",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "pg": "^8.18.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bun": "latest",
    "@types/cookie": "^0.6.0",
    "@types/formidable": "^3.4.6",
    "@types/mustache": "^4.2.6",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.20.18",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0"
  }
}
