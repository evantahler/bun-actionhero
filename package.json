{
  "name": "bun-actionhero",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "license": "MIT",
  "description": "Bun Actionhero - the greatest framework for building web applications, and other applications too!",
  "bin": {
    "actionhero": "actionhero.ts"
  },
  "scripts": {
    "migrations": "bun run migrations.ts",
    "build": "bun run build-backend && bun run build-frontend",
    "build-backend": "bun build actionhero.ts --compile --outfile actionhero",
    "build-frontend": "bun run next build",
    "lint": "prettier --check .",
    "pretty": "prettier --write .",
    "ci": "bun run lint && bun run build && bun test"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^12.1.0",
    "cookie": "^0.6.0",
    "drizzle-orm": "^0.30.10",
    "http-proxy": "^1.18.1",
    "ioredis": "^5.4.1",
    "next": "^14.2.7",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.8",
    "@types/http-proxy": "^1.17.15",
    "@types/cookie": "^0.6.0",
    "@types/formidable": "^3.4.5",
    "@types/pg": "^8.11.8",
    "@types/react-dom": "^18.3.0",
    "drizzle-kit": "^0.20.18",
    "prettier": "^3.3.3"
  }
}
